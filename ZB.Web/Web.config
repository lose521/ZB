<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <sectionGroup name="enyim.com">
      <section name="memcached" type="Enyim.Caching.Configuration.MemcachedClientSection, Enyim.Caching" />
    </sectionGroup>
    <section name="memcached" type="Enyim.Caching.Configuration.MemcachedClientSection, Enyim.Caching" />
  
  </configSections>
  
  <!--<enyim.com>
    <memcached configSource="WebConfig\enyim.com.config"/>
  </enyim.com>
  <memcached configSource="WebConfig\memcached.config"/>-->
  <enyim.com>
    <memcached protocol="Text">
        <servers>
            <add address="127.0.0.1" port="11211" />
        </servers>
        <socketPool minPoolSize="50" maxPoolSize="1000" connectionTimeout="00:01:10" deadTimeout="00:02:00" />
    </memcached>
  </enyim.com>
  <memcached keyTransformer="Enyim.Caching.TigerHashTransformer, Enyim.Caching">
    <servers>
        <add address="127.0.0.1" port="11211" />
    </servers>
    <socketPool minPoolSize="50" maxPoolSize="1000" connectionTimeout="00:01:10" deadTimeout="00:02:00" />
</memcached>
  <connectionStrings>
    <!--<add name="EFContext" connectionString="metadata=res://*/SqlServer.Model.csdl|res://*/SqlServer.Model.ssdl|res://*/SqlServer.Model.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.;initial catalog=ZB;persist security info=True;user id=sa;password=123456;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />-->
    <!--<add name="EFContext" connectionString="data source=.;initial catalog=ZB;persist security info=True;user id=sa;password=123456;multipleactiveresultsets=True;application name=EntityFramework" providerName="System.Data.SqlClient"/>-->
    <add name="EFContext" connectionString="data source=.;initial catalog=oa;persist security info=True;user id=sa;password=123;multipleactiveresultsets=True;application name=EntityFramework" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="AppName" value="ZB" />
    
  <!-- Enyim,DotNet,None -->
    <add key="CacheType" value="Enyim" />
    <add key="DotNetMemcachedServer" value="127.0.0.1:11211" />
    <add key="CachePoolName" value="ZB" />
    
  </appSettings>
